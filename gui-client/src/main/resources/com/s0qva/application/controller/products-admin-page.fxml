         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="558.0" layoutY="491.0" mnemonicParsing="false" onAction="#removeFromCart" text="Remove from the cart">
         <font>
            <Font size="14.0" />
