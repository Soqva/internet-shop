         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="351.0" layoutY="545.0" mnemonicParsing="false" onAction="#onCreateOrder" text="Оформить заказ" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <HBox fx:id="account" alignment="CENTER_RIGHT" layoutX="546.0" layoutY="19.0" prefHeight="30.0" prefWidth="155.0" spacing="30.0">
         <children>
            <Label fx:id="username" />
            <ImageView fx:id="userPhoto" fitHeight="29.0" fitWidth="60.0" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../image/default-user-photo.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </children>
</AnchorPane>
